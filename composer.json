{
    "name": "stakeholderlabs/roundtable-laravel",
    "description": "Package for integrating Laravel with Stakeholderlabs Roundtable",
    "license": "MIT",
    "require": {
        "illuminate/support": "~v8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "ext-json": "*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Shl\\RoundTable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shl\\RoundTable\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shl\\RoundTable\\Providers\\ServiceProvider"
            ]
        }
    }
}
